#include <iostream>

using namespace std;

int main()
{
    int savings;
    cin>>savings;
    if(savings>5000){
        if(savings>10000){
            cout<<"going on road trip";
        }
        else if(savings>7000){
            cout<<"going to cafe";
            
        }
        else{
            cout<<"goin to oyo :)";
        }
    }
    else if(savings>2000){
        cout<<"going with anushka\n";
    }
    else{
        cout<<"going with friends";
    }

    return 0;
}
