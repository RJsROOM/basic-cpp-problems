#include <iostream>
#include<climits>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int array[n];
    for(int i=0;i<n;i++){
        cin>>array[i];
    }
    int maxno= INT_MIN;
    int minno= INT_MAX;
    for(int i=0;i<n;i++){
        maxno=max(maxno,array[i]);
        minno=min(minno,array[i]);
              // if(array[i]>maxno){
                    //maxno=array[i];
                //}
               // if(array[i]<minno){
                   // minno=array[i];
                //}
                // THIS CODE CAN ALSO BE USED INPLACE OF THIS ABOVE ONE.
    }
    cout<<maxno<<" "<<minno;
    

    return 0;
}
